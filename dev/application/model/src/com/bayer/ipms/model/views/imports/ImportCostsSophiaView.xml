  <SQLQuery>
    <![CDATA[SELECT ImportCost.METHOD_CODE,
       CalculationMethod.NAME AS METHOD_NAME,
       ImportCost.STUDY_ID,
       ImportCost.SCOPE_CODE,
       CostsScope.NAME AS SCOPE_NAME,
       ImportCost.YEAR,
       ImportCost.MONTH,
       ImportCost.COST,
       ImportCost.STATUS_CODE,
       ImportStatus.NAME AS STATUS_NAME,
       ImportCost.SUBFUNCTION_CODE,
       SubFunction.NAME AS SUBFUNCTION_NAME,
       ImportCost.SUBTYPE_CODE,
       CostsSubType.NAME SUBTYPE_NAME,
       ImportCost.TYPE_CODE,
       CostsType.NAME AS TYPE_NAME,
       ImportCost.CREATE_DATE,
       ImportCost.IMPORT_ID,
       ImportCost.REFERENCE_ID,
       ImportCost.PROJECT_ID,
       ImportCost.STATUS_DESCRIPTION
FROM (
SELECT reference_id, import_id, project_id, study_id, subfunction_code, type_code, subtype_code, scope_code, method_code, year, month, cost, status_code, status_description, create_date FROM IMPORT_COSTS_GED
UNION ALL
SELECT reference_id, import_id, project_id, study_id, subfunction_code, type_code, subtype_code, scope_code, method_code, year, month, cost, status_code, status_description, create_date FROM IMPORT_COSTS_CS
) ImportCost
JOIN COSTS_SCOPE CostsScope ON ImportCost.SCOPE_CODE = CostsScope.CODE
JOIN CALCULATION_METHOD CalculationMethod ON ImportCost.METHOD_CODE = CalculationMethod.CODE
JOIN COSTS_TYPE CostsType ON ImportCost.TYPE_CODE = CostsType.CODE
JOIN IMPORT_STATUS ImportStatus ON ImportCost.STATUS_CODE = ImportStatus.CODE
LEFT JOIN COSTS_SUBTYPE CostsSubType ON ImportCost.SUBTYPE_CODE = CostsSubType.CODE
LEFT JOIN SUBFUNCTION SubFunction ON ImportCost.SUBFUNCTION_CODE = SubFunction.CODE
ORDER BY ImportCost.CREATE_DATE desc]]>
  </SQLQuery>
