  <SQLQuery><![CDATA[select     
    id,
    project_id,
    goal,
    type,
    phase,
    study_id,
    plan_reference,
    plan_reference_date,
    status,
    target_date,
    achieved_date,
    revised_date,
    comments,
    target_year
   from (
  select
    id,
    project_id,
    goal,
    type,
    phase,
    study_id,
    plan_reference,
    plan_reference_date,
    status,
    target_date,
    achieved_date,
    revised_date,
    comments,
    target_year,
    listagg(target_year, ',')
    within group (
      order by target_year)
    over (
      partition by project_id ) available_target_years
  from goal_vw
)
where (target_year=nvl(:SearchTargetYearVar,target_year) or instr(available_target_years,:SearchTargetYearVar)=0 and :SearchTargetYearVar=extract (year from sysdate))]]></SQLQuery>
