  <SQLQuery><![CDATA[select
	tm.project_id,
	tm.timeline_id,
	tm.milestone_code,
	tm.plan_date,
	tm.actual_date,
	ml.type_code as milestone_type_code,
	ml.name as milestone_name,
	reg.code as region_code,
	reg.name as region_name,
	substr(ml.code,length(reg.code)+2) region_type_code,
	ml.is_active is_active
from milestone_vw tm
join milestone ml on ml.code=tm.milestone_code
left join region reg on tm.milestone_code like reg.code||'-%'
where tm.timeline_type_code='CUR'
order by ml.ordering]]></SQLQuery>
