 ( select estimates_pkg.get_latest_fc_numbver as fc_numbver from dual )         
SELECT LtcTag.ID,           
       LtcTag.NAME,           
       LtcTag.START_YEAR,           
       LtcTag.IS_FROZEN,           
       LtcTag.IS_FORECAST_PROB,           
       LtcTag.SUBMIT_REPORT_DATE,           
