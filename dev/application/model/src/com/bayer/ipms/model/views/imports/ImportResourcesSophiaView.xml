  <SQLQuery>
    <![CDATA[SELECT ImportResources.REFERENCE_ID,
       ImportResources.IMPORT_ID,
       ImportResources.PROJECT_ID,
       ImportResources.STUDY_ID, 
       ImportResources.SUBFUNCTION_CODE, 
       SubFunction.NAME AS SUBFUNCTION_NAME,
       ImportResources.TYPE_CODE,
       ResourcesType.NAME AS TYPE_NAME, 
       ImportResources.METHOD_CODE,
       CalculationMethod.NAME AS METHOD_NAME, 
       ImportResources.YEAR, 
       ImportResources.MONTH, 
       ImportResources.DEMAND, 
       ImportResources.STATUS_CODE, 
       ImportStatus.NAME STATUS_NAME, 
       ImportResources.STATUS_DESCRIPTION,
       ImportResources.CREATE_DATE
FROM (
SELECT reference_id, import_id, project_id, study_id, subfunction_code, type_code, method_code, year, month, DEMAND, status_code, status_description, create_date FROM IMPORT_RESOURCES_GED
UNION ALL
SELECT reference_id, import_id, project_id, study_id, subfunction_code, type_code, method_code, year, month, DEMAND, status_code, status_description, create_date FROM IMPORT_RESOURCES_CS
) ImportResources
JOIN CALCULATION_METHOD CalculationMethod ON ImportResources.METHOD_CODE = CalculationMethod.CODE
JOIN RESOURCES_TYPE ResourcesType ON ImportResources.TYPE_CODE = ResourcesType.CODE
LEFT JOIN SUBFUNCTION SubFunction ON ImportResources.SUBFUNCTION_CODE = SubFunction.CODE
JOIN IMPORT_STATUS ImportStatus ON ImportResources.STATUS_CODE = ImportStatus.CODE
ORDER BY ImportResources.CREATE_DATE desc]]>
  </SQLQuery>
