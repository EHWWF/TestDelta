  <SQLQuery><![CDATA[SELECT 
       GlobalBALog.ID,   
       BusinessActivities.NAME,
       GlobalBALog.CREATE_DATE as flt_date_from,           
       GlobalBALog.PROGRAM_ID,                
       GlobalBALog.PROJECT_CODE,   
       GlobalBALog.status_code,
       GlobalBALog.CREATE_DATE as flt_date_to,               
       GlobalBALog.PROJECT_NAME,  
       GlobalBALog.PROGRAM_NAME, 
       GlobalBALog.PROJECT_ID,    
       GlobalBALog.BA_CODE,
       GlobalBALog.LOG_USER_ID,                        
       GlobalBALog.CREATE_DATE, 
       GlobalBALog.change_comment,
       GlobalBALog.description
FROM  GLOBAL_BA_LOG_VW GlobalBALog                      
JOIN  BUSINESS_ACTIVITY BusinessActivities on BusinessActivities.CODE = GlobalBALog.ba_code          
WHERE (:currActivity is null or :currActivity in ( cast (GlobalBALog.BAC as nvarchar2(3)) , GlobalBALog.BA_CODE))
and (:programNameVar is null or lower(program_name) like lower('%'||:programNameVar||'%'))
and (:projectNameVar is null or lower(project_name) like lower('%'||:projectNameVar||'%'))
and GlobalBALog.status_code is not null
 
 ORDER BY GlobalBALog.CREATE_DATE DESC]]></SQLQuery>
