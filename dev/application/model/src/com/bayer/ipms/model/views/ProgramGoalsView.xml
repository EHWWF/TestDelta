  <SQLQuery><![CDATA[select  
  ID,  
  PROJECT_ID,  
  GOAL,  
  TYPE,  
  STUDY_ID,  
  PLAN_REFERENCE,  
  STATUS,  
  TARGET_DATE,  
  ACHIEVED_DATE,  
  REVISED_DATE,  
  COMMENTS,  
  Program_id,  
  code,  
  PLAN_REFERENCE_DATE,  
  TARGET_YEAR,  
  TARGET_YEAR TARGET_YEAR_CHECK,  
  is_manual_status  
  from (  
    select  
      goal.id,  
      goal.project_id,  
      goal.goal,  
      goal.type,  
      goal.study_id,  
      goal.plan_reference,  
      goal.status,  
      goal.target_date,  
      goal.achieved_date,  
      goal.revised_date,  
      goal.comments,  
      goal.is_manual_status,  
      project.program_id,  
      project.code,  
      goal.plan_reference_date,  
      goal.target_year,  
      listagg(target_year, ',')  
      within group (  
        order by target_year)  
      over (  
        partition by project.program_id ) available_target_years  
    from goal_vw goal  
      join project project on goal.project_id = project.id  
  )  
  where (target_year=nvl(:SearchTargetYearVar,target_year) or instr(available_target_years,:SearchTargetYearVar)=0 and :SearchTargetYearVar=extract (year from sysdate))]]></SQLQuery>
