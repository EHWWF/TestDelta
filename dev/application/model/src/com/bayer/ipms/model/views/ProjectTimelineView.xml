  <SQLQuery><![CDATA[select Timeline.ID,      
       Timeline.NAME,      
       Timeline.PROJECT_ID,      
       Timeline.TYPE_CODE,      
       Timeline.CREATE_DATE,      
       Timeline.UPDATE_DATE,      
       Timeline.SYNC_DATE,      
       Timeline.IS_SYNCING,      
       timeline.COMMENTS,      
       TimelineType.NAME AS TYPE_NAME,      
       TimelineType.CODE AS TYPE_CODE_2,      
       Timeline.REFERENCE_ID,      
       Timeline.START_DATE,      
       Timeline.FINISH_DATE,      
       Timeline.SYNC_ID,      
       Project.ID AS ID1,      
       nvl(Project.NAME, Timeline.NAME) as NAME1,      
       nvl(programsandbox.program_id, Project.PROGRAM_ID) as program_id,     
       nvl(Project.CODE, programsandbox.code) as CODE,     
       project.description,     
       project.program_id prj_prg_id,      
       decode(project.is_active, 0,'(inactive)',null) project_is_active,     
       programsandbox.id sandbox_id,     
       programsandbox.code as sand_code,     
       programsandbox.name as sand_name,     
       programsandbox.program_id as prg_id     
from timeline timeline     
join timeline_type timelinetype on (timeline.type_code = timelinetype.code)     
left join project project on (timeline.project_id = project.id)      
left join program_sandbox programsandbox on (timeline.id = programsandbox.snd_timeline_id) and programsandbox.is_active = 1]]></SQLQuery>
