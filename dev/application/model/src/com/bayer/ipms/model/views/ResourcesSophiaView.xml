  <SQLQuery>
    <![CDATA[SELECT Resources.ID,
       Resources.PROJECT_ID,
       Resources.STUDY_ID,
       Resources.FUNCTION_CODE,
       FunctionData.NAME FUNCTION_NAME,
       Resources.METHOD_CODE,
       CalculationMethod.NAME AS METHOD_NAME,
       Resources.TYPE_CODE,
       ResourcesType.NAME AS TYPE_NAME,
       Resources.SUBFUNCTION_CODE,
       SubFunction.NAME AS SUBFUNCTION_NAME,
       Resources.DEMAND,
       Resources.START_DATE,
       Resources.FINISH_DATE,
       Resources.CREATE_DATE,
       Resources.UPDATE_DATE
FROM (
SELECT id, project_id, study_id, FUNCTION_CODE, subfunction_code, method_code, type_code, DEMAND, start_date, finish_date, create_date, update_date FROM RESOURCES_GED
UNION ALL
SELECT id, project_id, study_id, FUNCTION_CODE, subfunction_code, method_code, type_code, DEMAND, start_date, finish_date, create_date, update_date FROM RESOURCES_CS
) Resources
LEFT JOIN FUNCTION FunctionData ON Resources.FUNCTION_CODE = FunctionData.CODE
LEFT JOIN SUBFUNCTION SubFunction ON Resources.SUBFUNCTION_CODE = SubFunction.CODE
JOIN RESOURCES_TYPE ResourcesType ON Resources.TYPE_CODE = ResourcesType.CODE
JOIN CALCULATION_METHOD CalculationMethod ON Resources.METHOD_CODE = CalculationMethod.CODE]]>
  </SQLQuery>
