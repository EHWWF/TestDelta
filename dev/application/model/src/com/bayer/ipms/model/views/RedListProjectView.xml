    <SchemaBasedProperties>
      <LABEL
        ResId="com.bayer.ipms.model.views.RedListProjectView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[select 
    prj.id,
    prj.code,
    prj.name,
    sbe.name sbe_name ,
    prj.current_comment,
    prj.successor_project_id
from project prj
join project base_prj on base_prj.id=prj.id
left join strategic_business_entity sbe on sbe.code = prj.sbe_code
WHERE UPPER(prj.planning_enabled) = 'RED'
order by name]]></SQLQuery>
