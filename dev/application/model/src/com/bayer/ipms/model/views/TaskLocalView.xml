  <SQLQuery><![CDATA[select
  tsk.id,
  tsk.task_number,
  tsk.definition_name,
  tsk.title,
  tsk.comments,
  tsk.state,
  tsk.task_created_date,
  tsk.expiration_date,
  tsk.outcome,
  tsk.priority,
  tsk.assumption_request_id,
  tsk.process_id,
  tsk.program_id,
  tsk.project_id
from task tsk
where
  tsk.definition_name='ProvideLatestEstimates1' and (:IsEstimatesProvide='true' or :IsEstimatesProvideAssigned='true' and  exists(select 1 from user_role where UPPER(user_id)=UPPER(:UserName) and program_id=tsk.program_id))
  or tsk.definition_name='ProvideLTC' and (:IsEstimatesProvideLTC='true' or :IsEstimatesProvideAssignedLTC='true' and  exists(select 1 from user_role where UPPER(user_id)=UPPER(:UserName) and program_id=tsk.program_id))
  or tsk.definition_name='ProvidePlanningAssumptions' and (:IsPlanningAssumptionsProvide='true' or :IsPlanAssumptProvideAssigned='true')
  or tsk.definition_name='AssignProjectID' and :IsProjectEditIdDev='true']]></SQLQuery>
