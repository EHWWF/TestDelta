  <SQLQuery><![CDATA[select                           
	LtcPlan.ID,                            
	LtcPlan.LTCI_ID,                            
	LtcPlan.CODE,                            
	LtcPlan.NAME,                            
	LtcPlan.STUDY_ID,                            
	LtcPlan.WBS_ID,                            
	LtcPlan.PARENT_WBS_ID,                            
	LtcPlan.CREATE_DATE,                            
	LtcPlan.START_DATE,                            
	LtcPlan.FINISH_DATE,                          
        LtcPlan.INT_EXT_FLAG,                           
	LtcPlan.STUDY_MODUS_NO,                            
	LtcPlan.STUDY_MODUS_NAME,                           
	LtcPlan.CLIN_PLAN_TYPE,                           
	LtcPlan.STUDY_UNIT_COUNT,                      
	LtcPlan.actual_patients,                     
	LtcPlan.plan_patients,                     
	LtcPlan.study_unit_count_plan,                     
	round(LtcPlan.lt_int_cost_total, 3) lt_int_cost,                           
	round(LtcPlan.lt_cro_cost_total, 3) lt_cro_cost,                           
	round(LtcPlan.lt_ecg_cost_total, 3) lt_ecg_cost,                           
	round(LtcPlan.lt_oec_cost_total, 3) lt_oec_cost,                           
	round(LtcPlan.act_int_cost_total, 3) act_int_cost,                           
	round(LtcPlan.act_cro_cost_total, 3) act_cro_cost,                           
	round(LtcPlan.act_ecg_cost_total, 3) act_ecg_cost,                           
	round(LtcPlan.act_oec_cost_total, 3) act_oeci_cost,                           
	round(LtcPlan.fc_int_cost_total, 3) fc_int_cost,                           
	round(LtcPlan.fc_cro_cost_total, 3) fc_cro_cost,                           
	round(LtcPlan.fc_ecg_cost_total, 3) fc_ecg_cost,                           
	round(LtcPlan.fc_oec_cost_total, 3) fc_oeci_cost,      
        decode( nvl(LtcPlan.WBS_ID,'*'),'*' ,1 ,0) as total_flag      
from ltc_plan LtcPlan where                   
                   
-- Forecast exists                          
(LtcPlan.id in                           
(select ltcp2.id from ltc_plan ltcp2 connect by prior ltcp2.parent_wbs_id =  ltcp2.wbs_id and ltcp2.ltci_id=ltcplan.ltci_id start with (ltcp2.fc_int_cost_total is not null or ltcp2.fc_cro_cost_total is not null or	ltcp2.fc_ecg_cost_total is not null or ltcp2.fc_oec_cost_total is not null)  and ltcp2.ltci_id=ltcplan.ltci_id )                          
 or :SearchFcExistsVar is null                           
 or :SearchFcExistsVar ='false')                          
                          
-- LTC values are provided                          
and (LtcPlan.id in                           
(select ltcp2.id from ltc_plan ltcp2 connect by prior ltcp2.parent_wbs_id =  ltcp2.wbs_id and ltcp2.ltci_id=ltcplan.ltci_id start with exists (select 1 from ltc_value ltcv2 where ltcv2.ltcp_id=ltcp2.id) and ltcp2.ltci_id=ltcplan.ltci_id)                          
 or :SearchLtcvExistsVar is null                           
 or :SearchLtcvExistsVar ='false')                          
                          
-- Study related WBS nodes                          
and (LtcPlan.id in                           
(select ltcp2.id from ltc_plan ltcp2 connect by prior ltcp2.parent_wbs_id =  ltcp2.wbs_id and ltcp2.ltci_id=ltcplan.ltci_id start with ltcp2.study_id is not null and ltcp2.ltci_id=ltcplan.ltci_id)                          
 or :SearchStudyRelatedVar is null                           
 or :SearchStudyRelatedVar ='false')                          
                           
 --Exclude totals when filter is applied                          
 minus                          
 select 	                   
	LtcPlan.ID,                            
	LtcPlan.LTCI_ID,                            
	LtcPlan.CODE,                            
	LtcPlan.NAME,                            
	LtcPlan.STUDY_ID,                            
	LtcPlan.WBS_ID,                            
	LtcPlan.PARENT_WBS_ID,                            
	LtcPlan.CREATE_DATE,                            
	LtcPlan.START_DATE,                            
	LtcPlan.FINISH_DATE,                          
        LtcPlan.INT_EXT_FLAG,                           
	LtcPlan.STUDY_MODUS_NO,                            
	LtcPlan.STUDY_MODUS_NAME,                           
	LtcPlan.CLIN_PLAN_TYPE,                           
	LtcPlan.STUDY_UNIT_COUNT,                      
	LtcPlan.actual_patients,                     
	LtcPlan.plan_patients,                     
	LtcPlan.study_unit_count_plan,                     
	LtcPlan.lt_int_cost_total,                           
	LtcPlan.lt_cro_cost_total,                           
	LtcPlan.lt_ecg_cost_total,                           
	LtcPlan.lt_oec_cost_total,                           
	LtcPlan.act_int_cost_total,                           
	LtcPlan.act_cro_cost_total,                           
	LtcPlan.act_ecg_cost_total,                           
	LtcPlan.act_oec_cost_total,                           
	LtcPlan.fc_int_cost_total,                           
	LtcPlan.fc_cro_cost_total,                           
	LtcPlan.fc_ecg_cost_total,                           
	LtcPlan.fc_oec_cost_total,  
        decode( nvl(LtcPlan.WBS_ID,'*'),'*' ,1 ,0) as total_flag   
	from ltc_plan LtcPlan where LtcPlan.wbs_id is null and( :SearchStudyRelatedVar='true' or :SearchLtcvExistsVar='true' or :SearchFcExistsVar='true')           
order by total_flag, NAME]]></SQLQuery>
