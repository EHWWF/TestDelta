  <SQLQuery><![CDATA[with study_data as (select * from study_data_vw where TIMELINE_TYPE_CODE in ('CUR','RAW') and project_id=:ProjectIdVar)      
select      
  nvl(straw.id,stcur.id) id,--+      
  nvl(straw.wbs_id,stcur.wbs_id) wbs_id,      
  nvl(straw.code,stcur.code) code,      
      
  nvl(straw.name,stcur.name) name,--+      
  case when straw.name != stcur.name then 1 else 0 end is_name_discrep,      
      
  nvl(straw.project_id,stcur.project_id) project_id,      
      
  nvl(straw.status_code,stcur.status_code) status_code,      
      
  nvl(rawsta.name,cursta.name) status_name,--+      
  case when rawsta.code != cursta.code then 1 else 0 end is_status_discrep,      
      
  nvl(straw.finish_date,stcur.finish_date) finish_date,--+      
  case when straw.finish_date != stcur.finish_date then 1 else 0 end is_finish_date_discrep,      
      
  nvl(straw.fpfv,stcur.fpfv) fpfv,--+      
  case when straw.fpfv != stcur.fpfv then 1 else 0 end is_fpfv_discrep,      
        
  nvl(straw.lplv,stcur.lplv) lplv,--+      
  case when straw.lplv != stcur.lplv then 1 else 0 end is_lplv_discrep,      
        
  nvl(straw.timeline_id,stcur.timeline_id) timeline_id,      
      
  nvl(straw.phase,stcur.phase) phase,--+      
  case when straw.phase != stcur.phase then 1 else 0 end is_phase_discrep,      
      
  nvl(straw.start_date,stcur.start_date) start_date,--+      
  case when straw.start_date != stcur.start_date then 1 else 0 end is_start_date_discrep,      
      
  nvl(straw.timeline_type_code,stcur.timeline_type_code) timeline_type_code,      
      
  nvl(straw.is_gpdc_approved,stcur.is_gpdc_approved) is_gpdc_approved,--+      
  case when straw.is_gpdc_approved != stcur.is_gpdc_approved then 1 else 0 end is_gpdc_approved_discrep,      
      
  nvl(straw.is_obligation,stcur.is_obligation) is_obligation,--+      
  case when straw.is_obligation != stcur.is_obligation then 1 else 0 end is_obligation_discrep,      
      
  nvl(straw.is_probing,stcur.is_probing) is_probing,--+      
  case when straw.is_probing != stcur.is_probing then 1 else 0 end is_probing_discrep,      
      
  nvl(straw.placeholder,stcur.placeholder) placeholder,--+      
  case when straw.placeholder != stcur.placeholder then 1 else 0 end is_placeholder_discrep,      
      
  nvl(straw.dmc_actual,stcur.dmc_actual) dmc_actual,--+      
  case when straw.dmc_actual != stcur.dmc_actual then 1 else 0 end is_dmc_actual_discrep,      
      
  nvl(straw.dmc_plan,stcur.dmc_plan) dmc_plan,--+      
  case when straw.dmc_plan != stcur.dmc_plan then 1 else 0 end is_dmc_plan_discrep,      
  nvl(straw.wbs_count,stcur.wbs_count) wbs_count,      
      
  nvl(straw.plan_patients,stcur.plan_patients) plan_patients,--+      
  case when straw.plan_patients != stcur.plan_patients then 1 else 0 end is_plan_patients_discrep,      
      
  nvl(straw.actual_patients,stcur.actual_patients) actual_patients,--+      
  case when straw.actual_patients != stcur.actual_patients then 1 else 0 end is_actual_patients_discrep,      
      
  nvl(straw.project_id,stcur.project_id) project_id1,      
      
  nvl(straw.timeline_id,stcur.timeline_id) timeline_id1,      
      
  nvl(straw.update_date,stcur.update_date) update_date,      
      
  nvl(straw.therapeutic_group_code,stcur.therapeutic_group_code) therapeutic_group_code,      
      
  nvl(straw.therapeutic_group_desc,stcur.therapeutic_group_desc) therapeutic_group_desc,      
  case when straw.therapeutic_group_desc != stcur.therapeutic_group_desc then 1 else 0 end is_ther_group_desc_discrep,      
      
  nvl(straw.int_ext_flag,stcur.int_ext_flag) int_ext_flag,--+      
  case when straw.int_ext_flag != stcur.int_ext_flag then 1 else 0 end is_int_ext_flag_discrep,      
      
  nvl(straw.is_timeline_auto_import,stcur.is_timeline_auto_import) is_timeline_auto_import,--+      
  case when straw.is_timeline_auto_import != stcur.is_timeline_auto_import then 1 else 0 end is_timeline_auto_imp_discrep,      
      
  nvl(straw.act_entered_screen,stcur.act_entered_screen) act_entered_screen,--+      
  case when straw.act_entered_screen != stcur.act_entered_screen then 1 else 0 end is_act_entered_screen_discrep,      
      
  nvl(straw.plan_entered_screen,stcur.plan_entered_screen) plan_entered_screen,--+      
  case when straw.plan_entered_screen != stcur.plan_entered_screen then 1 else 0 end is_plan_entered_screen_discrep,      
      
  nvl(straw.study_country_count,stcur.study_country_count) study_country_count,--+      
  case when straw.study_country_count != stcur.study_country_count then 1 else 0 end is_study_country_count_discrep,      
      
  nvl(straw.study_country_count_plan,stcur.study_country_count_plan) study_country_count_plan,--+      
  case when straw.study_country_count_plan != stcur.study_country_count_plan then 1 else 0 end is_country_count_plan_discrep,      
      
  nvl(straw.study_unit_count,stcur.study_unit_count) study_unit_count,--+      
  case when straw.study_unit_count != stcur.study_unit_count then 1 else 0 end is_study_unit_count_discrep,      
      
  nvl(straw.study_unit_count_plan,stcur.study_unit_count_plan) study_unit_count_plan,--+      
  case when straw.study_unit_count_plan != stcur.study_unit_count_plan then 1 else 0 end is_unit_count_plan_discrep,      
      
  nvl(straw.is_lead,stcur.is_lead) is_lead,--+      
  case when straw.is_lead != stcur.is_lead then 1 else 0 end is_lead_discrep,      
      
  nvl(straw.cdblock,stcur.cdblock) cdblock,--+      
  case when straw.cdblock != stcur.cdblock then 1 else 0 end is_cdblock_discrep,      
      
  nvl(straw.csrapp,stcur.csrapp) csrapp,--+      
  case when straw.csrapp != stcur.csrapp then 1 else 0 end is_csrapp_discrep,      
      
  nvl(straw.prcompl,stcur.prcompl) prcompl,      
  case when straw.prcompl != stcur.prcompl then 1 else 0 end is_prcompl_discrep,      
      
  nvl(straw.is_delete,stcur.is_delete) is_delete,      
      
  nvl(straw.budget_class,stcur.budget_class) budget_class,      
  case when straw.budget_class != stcur.budget_class then 1 else 0 end is_budget_class_discrep,      
      
  nvl2(straw.id,1,0) is_in_raw_plan,      
  nvl2(stcur.id,1,0) is_in_cur_plan,      
      
  --nvl2(straw.id,nvl2(stcur.id,'RAW|CUR','RAW'),'CUR') search_plan,      
      
  nvl(straw.study_modus_name,stcur.study_modus_name) study_modus_name,--+      
  case when straw.study_modus_name != stcur.study_modus_name then 1 else 0 end is_study_modus_name_discrep,      
        
  nvl(straw.fpfvb,stcur.fpfvb) fpfvb,--+      
  case when straw.fpfvb != stcur.fpfvb then 1 else 0 end is_fpfvb_discrep,      
        
  nvl(straw.lplva,stcur.lplva) lplva,--+      
  case when straw.lplva != stcur.lplva then 1 else 0 end is_lplva_discrep,      
    
  nvl(straw.study_modus_no,stcur.study_modus_no) study_modus_no    
    
from (select * from study_data where TIMELINE_TYPE_CODE='RAW') straw      
  left join study_status rawsta on straw.status_code = rawsta.code      
      
  full outer join (select * from study_data where TIMELINE_TYPE_CODE='CUR' ) stcur      
    on straw.id = stcur.id      
  left join study_status cursta on stcur.status_code = cursta.code      
      
where      
  (:SearchPlanCodeVar is null or nvl2(straw.id,nvl2(stcur.id,'RAW|CUR','RAW'),'CUR')=:SearchPlanCodeVar or nvl2(straw.id,nvl2(stcur.id,'RAW|CUR','RAW'),'CUR')='RAW|CUR' )      
  and (:SearchPlaceholderVar is null or nvl(straw.placeholder,stcur.placeholder)=decode(:SearchPlaceholderVar,'true',1,0) or :SearchPlaceholderVar='false')      
  -- filtering for straw removal    
  and ( 0=:StudyRemoveVar or 1=:StudyRemoveVar      
                             and not      
                             (exists(select 1      
                                     from costs_fps fps      
                                     where fps.study_id = straw.id       
									 and fps.project_id=straw.project_id      
									 and extract(year from fps.start_date) >= extract(year from sysdate)      
									 ) or      
                              exists(select 1      
                                     from costs cs      
                                     where cs.study_id = straw.id       
									 and cs.project_id=straw.project_id      
									 and extract(year from cs.start_date) >= extract(year from sysdate)      
									 )))   
order by nvl(straw.wbs_count,stcur.wbs_count), nvl(straw.id,stcur.id)]]></SQLQuery>
