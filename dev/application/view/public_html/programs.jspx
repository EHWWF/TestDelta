            <af:resource type="javascript">
              function onTreeSelect(event) {
                  var _tree = event.getSource();
                  var rwKeySet = event.getAddedSet();
                  var firstRowKey;
                  for (rowKey in rwKeySet) {
                      firstRowKey = rowKey;
                      break;
                  }
                  _tree.setDisclosedRowKey(firstRowKey, true);
              }
              
              
              //stops the polling after first call
              function resetPollInterval(e) {
                var src = e.getSource();
                src.setInterval( - 1);
              }
