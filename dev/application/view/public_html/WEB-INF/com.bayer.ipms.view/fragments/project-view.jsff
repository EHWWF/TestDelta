          function removeFocus(event) {
              if (document.activeElement)
                  document.activeElement.blur();
          }

          function onCommentsChange(event) {
              var src = event.getSource();
              var trg = src.getParent().findComponent("pubComment");
              trg.setValue(src.getValue());
          }
        </af:resource>
        <af:resource type="css">
            .af_selectOneChoice_content {
                width: 75%;
            }

            .af_selectBooleanCheckbox_item-text, .af_selectBooleanCheckbox_content {
                color: rgb(117, 86, 0);
            }
        </af:resource>
