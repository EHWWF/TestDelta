                            <af:declarativeComponent id="changes" viewId="project-changes.jsff"/>
                        </af:showDetailItem>
                        <af:showDetailItem text="#{bundle.projectLevelActivities}" stretchChildren="first"
                                           immediate="true"
                                           disclosureListener="#{pageFlowScope.projectViewBean.tabDisclosure}"
                                           id="sdiProjectLevelActivities"
                                           rendered="#{rolesBean.activityBasedPlanningViewDev}"
                                           disabled="#{not empty pageFlowScope.projectViewBean.editable or pageFlowScope.projectViewBean.modalMode}">
                            <af:declarativeComponent id="project-level-activities"
                                                     viewId="project-level-activities.jsff"/>
                        </af:showDetailItem>
                    </af:panelTabbed>
                </f:facet>
            </af:panelStretchLayout>
        </f:facet>
        <f:attribute name="title" value="#{bundle.project} #{bindings.Name.inputValue}"/>
        <f:attribute name="message"
                     value="#{bindings.IsSyncing.inputValue ? bundle.globalIsSyncingWarning : bindings.IsRunningImport.inputValue eq 'YES'?bundle.globalImportRunning:''}"/>
    </af:pageTemplate>
</jsp:root>
