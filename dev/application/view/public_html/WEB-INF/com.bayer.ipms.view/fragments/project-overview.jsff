                                                 viewId="project-pip.jsff"/>
                        <af:declarativeComponent id="licenses"
                                                 rendered="#{bindings.AreaCode eq 'PRE-POT' or bindings.AreaCode eq 'POST-POT'}"
                                                 viewId="project-licenses.jsff"/>
                        <af:declarativeComponent id="api" rendered="#{bindings.AreaCode ne 'SAMD'}" viewId="project-ingredients.jsff"/>
                        <af:declarativeComponent id="RoadMap" rendered="#{bindings.AreaCode eq 'SAMD'}"
                                                 viewId="project-roadmap.jsff"/>
                        <af:subform id="regulatory"
                                    rendered="#{bindings.AreaCode eq 'PRE-POT' or bindings.AreaCode eq 'POST-POT'}">
                            <af:panelBox text="#{bundle.projectRegulatory}" showDisclosure="false"
                                         background="#{pageFlowScope.projectViewBean.editable eq 'regulatory' ? 'light' : 'default'}"
                                         styleClass="AFStretchWidth">
                                <f:facet name="toolbar">
                                    <af:toolbar rendered="#{rolesBean.projectEditDev}">
                                        <af:commandToolbarButton icon="/images/edit.png"
                                                                 rendered="#{pageFlowScope.projectViewBean.editable ne 'regulatory'}"
                                                                 immediate="true"
                                                                 disabled="#{not empty pageFlowScope.projectViewBean.editable}"
                                                                 actionListener="#{pageFlowScope.projectViewBean.onEdit}"
                                                                 text="#{bundle.globalEdit}">
                                            <f:attribute name="target" value="regulatory"/>
                                        </af:commandToolbarButton>
                                        <af:commandToolbarButton rendered="#{pageFlowScope.projectViewBean.editable eq 'regulatory'}"
                                                                 icon="/images/commit.png"
                                                                 actionListener="#{pageFlowScope.projectViewBean.onCommit}"
                                                                 text="#{bundle.globalOk}"/>
                                        <af:commandToolbarButton rendered="#{pageFlowScope.projectViewBean.editable eq 'regulatory'}"
                                                                 icon="/images/rollback.png"
                                                                 actionListener="#{pageFlowScope.projectViewBean.onRollback}"
                                                                 immediate="true" partialSubmit="true"
                                                                 text="#{bundle.globalCancel}"/>
                                    </af:toolbar>
                                </f:facet>
                                <af:panelFormLayout id="pfl41" maxColumns="1" rows="8" styleClass="AFStretchWidth"
                                                    fieldWidth="100%" partialTriggers="">
                                    <af:selectManyChoice id="smcReg"
                                                         value="#{bindings.ProjectViewIterator.currentRow.regulatoryCodes}"
                                                         label="#{bindings.RegulatoryCode.label}"
                                                         required="#{bindings.RegulatoryCode.hints.mandatory}"
                                                         shortDesc="#{bindings.RegulatoryCode.hints.tooltip}"
                                                         rendered="#{bindings.AreaCode eq 'PRE-POT' or bindings.AreaCode eq 'POST-POT'}"
                                                         readOnly="#{pageFlowScope.projectViewBean.editable ne 'regulatory'}"
                                                         contentStyle="white-space: normal;">
                                        <f:selectItems value="#{utilsBean.lookup[bindings.RegulatoryTypeViewIterator.rowSetIterator]}"
                                                       id="siReg"/>
                                        <f:validator binding="#{bindings.RegulatoryCode.validator}"/>
                                    </af:selectManyChoice>
                                    <af:inputText value="#{bindings.RegulatoryOther.inputValue}"
                                                  label="#{bindings.RegulatoryOther.hints.label}"
                                                  required="#{bindings.RegulatoryOther.hints.mandatory}"
                                                  maximumLength="#{bindings.RegulatoryOther.hints.precision}"
                                                  readOnly="#{pageFlowScope.projectViewBean.editable ne 'regulatory'}"
                                                  shortDesc="#{bindings.RegulatoryOther.hints.tooltip}" id="dc_it5"
                                                  rows="5" styleClass="AFStretchWidth">
                                        <f:validator binding="#{bindings.RegulatoryOther.validator}"/>
                                    </af:inputText>
                                </af:panelFormLayout>
                            </af:panelBox>
                        </af:subform>
                    </af:gridCell>
                    <af:gridCell width="100%" halign="stretch">
                        <af:declarativeComponent id="plans"
                                                 rendered="#{bindings.AreaCode eq 'PRE-POT' or bindings.AreaCode eq 'POST-POT' or bindings.AreaCode eq 'D2-PRJ' or bindings.AreaCode eq 'SAMD' or bindings.AreaCode eq 'D3-TR' or bindings.AreaCode eq 'PRD-MNT'}"
                                                 viewId="project-plans.jsff"/>
                        <af:declarativeComponent id="milestones"
                                                 rendered="#{bindings.AreaCode eq 'PRE-POT' or bindings.AreaCode eq 'POST-POT' or bindings.AreaCode eq 'D2-PRJ' or bindings.AreaCode eq 'SAMD' or bindings.AreaCode eq 'D3-TR'}"
                                                 viewId="project-milestones.jsff"/>
                    </af:gridCell>
                </af:gridRow>
            </af:panelGridLayout>
        </af:panelGroupLayout>
    </af:componentDef>
</jsp:root>
