                                                         icon="/images/rollback.png"  immediate="true" 
                                                         actionListener="#{pageFlowScope.topBean.onRollback}"
                                                         action="afterCommit"
                                                         text="#{bundle.globalCancel}" id="ctb_cn">
                                    <af:resetActionListener/>                     
                                </af:commandToolbarButton>                         
                            </af:toolbar>
